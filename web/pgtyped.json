{
  "transforms": [
    {
      "mode": "ts",
      "include": "**/*.{ts,tsx}",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    },
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.sql.ts"
    }
  ],
  "srcDir": "src",
  "typesOverrides": {
    "int8": "number",
    "timestamp": "string"
  }
}
